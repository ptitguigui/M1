import java.util.ArrayList;
import java.util.List;

public class Algorithms {
		
	public static void main(String[] args) {
		Point p1 = new Point(0,0);
		Point p2 = new Point(0,5);
		Point p3 = new Point(2,2);
		Point p4 = new Point(5,5);
		Point p5 = new Point(9,0);
		Point p6 = new Point(9,5);
		
		List<Point> list = new ArrayList<>();
		list.add(p1);
		list.add(p2);
		list.add(p3);
		list.add(p4);
		list.add(p5);
		list.add(p6);
	}

	static int calculerLongueur(Point p1, Point p2){
		return (int) Math.sqrt( Math.pow((p2.x - p1.x),2) + Math.pow((p2.y - p1.y),2));
	}
	
	static int grandRectangle(List<Point> list, int h, int l){
		int airMax = 0;
		int minOrdonne;
		
		Point p1;
		Point p2;
		
		for (Point point1 : list) {
			for (Point point2 : list) {
				if(!point1.equals(point2)){
					
				}
			}
		}
		
		return 0;
	}
	
	static int minOrdonne(Point point1, Point point2,List<Point> list){
		int ordonne = 1000000;
		for (Point point : list) {
			if(point.x > point1.x && point.x < point2.x){
				if(ordonne < point.y){
					ordonne = point.y;
				}
			}
		}
		return ordonne;
	}
	
	static int airRectangle(int x1, int x2, int ordonne){
		return (x2-x1)*ordonne;
	}
	
}

<?xml version="1.0" encoding="UTF-8"?>

<schema xmlns="http://www.w3.org/2001/XMLSchema"
 	xmlns:exm="http://www.exam.labd/"
 	targetNamespace="http://www.exam.labd/"
 	elementFormDefault="qualified">
  <element name="examen">
    <complexType>
      <sequence>
        <element maxOccurs="unbounded" ref="exm:exercice" />
      </sequence>
    </complexType>
  </element>
  
  <element name="exercice" type="string"/>

</schema>
